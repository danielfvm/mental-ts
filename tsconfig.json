{
  "$schema": "https://raw.githubusercontent.com/TypeScriptToLua/TypeScriptToLua/master/tsconfig-schema.json",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": [
      "lua-types/5.4"
    ],
    "strict": true
  },
  "tstl": {
    "luaTarget": "5.4",
    "luaLibImport": "inline"
  }
}
